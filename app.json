{
  "expo": {
    "name": "tukubkao",
    "slug": "tukubkao",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tukubkao",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.sut.tukubkao",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.973460455176-ntpvi36ac6sllvojas7ldgq93lepj96r"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.973460455176-ntpvi36ac6sllvojas7ldgq93lepj96r"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show you nearby places.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show you nearby places.",
        "UIBackgroundModes": ["location"]
      },
      "config": {
        "googleMapsApiKey": "AIzaSyD7jiKFEPqsMKvTEV0lVOnNj1Gcz1e-iRk"
      }
    },
    "android": {
      "package": "com.sut.tukubkao",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD7jiKFEPqsMKvTEV0lVOnNj1Gcz1e-iRk"
        }
      },
      "notification": {
        "icon": "./assets/notification-icon.png",
        "color": "#A5B68D"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F2E6"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F8F2E6"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow tukubkao to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "44e2fe2d-f476-406e-adc1-892593d6c122"
      }
    }
  }
}
